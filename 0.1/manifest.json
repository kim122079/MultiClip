{
   "name": "clipper",
   "description": "Clip multiple snippets.",
   "version": "0.1",
   "icons": {
      "32": "32.png"
   },
   "permissions": [
    "tabs", "http://*/*", "https://*/*"
   ],
   "background_page": "background.html",
   "browser_action": {
      "default_icon": "sample-16x16.png",
      "default_title": "Easy Clipboard",
      "popup" : "popup.html"
   },
   "content_scripts": [ {
      "css": [ "content.css" ],
      "js": [ "jquery-1.3.2.js", "content.js" ],
      "matches": [ "http://*/*", "https://*/*"]
   } ]
}
