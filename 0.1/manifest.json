{
   "name": "clipper",
   "description": "Clip multiple snippets.",
   "version": "0.1",
   "options_page": "options.html",
   "permissions": [
    "tabs", "http://*/*", "https://*/*"
   ],
   "background_page": "background.html",
   "browser_action": {
      "default_icon": "multiclipboard_icon_19x19.png",
      "default_title": "Multiclipboard",
      "popup" : "popup.html"
   },
   "content_scripts": [ {
      "css": [ "content.css" ],
      "js": [ "jquery-1.3.2.js", "content.js" ],
      "matches": [ "http://*/*", "https://*/*"]
   } ]
}
