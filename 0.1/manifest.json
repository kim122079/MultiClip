{
   "name": "Multiclip",
   "description": "Clip multiple text snippets.",
   "version": "0.1",
   "icons": { "48": "multiclip48x48.png",
              "128": "multiclip128x128.png" },   
   "permissions": [
    "tabs", "http://*/*", "https://*/*"
   ],
   "options_page": "options.html",
   "background_page": "background.html",
   "browser_action": {
      "default_icon": "multiclip16x16.png",
      "default_title": "Multiclipboard",
      "popup" : "popup.html"
   },
   "content_scripts": [ {
      "css": [ "content.css" ],
      "js": [ "jquery-1.3.2.js", "content.js" ],
      "matches": [ "http://*/*", "https://*/*"]
   } ]
}
